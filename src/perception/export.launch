<launch>
    <node pkg="rosbag" type="play" name="rosbag" required="true" args="$(find image_view)/bag-name.bag"/>
    <node name="extract" pkg="image_view" type="extract_images" respawn="false" required="true" output="screen" cwd="ROS_HOME">
        <remap from="image" to="/wamv/sensors/cameras/middle_right_camera/image_raw"/>
    </node>
</launch>
